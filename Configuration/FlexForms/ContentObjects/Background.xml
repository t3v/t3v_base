                  <numIndex index="9" type="array">
                    <numIndex index="0">LLL:EXT:t3v_base/Resources/Private/Language/locallang_ff.xlf:settings.backgroundPosition.centerBottom.label</numIndex>
                    <numIndex index="1">center bottom</numIndex>
                  </numIndex>
                </items>
                <default></default>
              </config>
            </TCEforms>
