/*
  ---------------------------------------
  | T3v Base Section Grid Element Setup |
  ---------------------------------------
*/

tt_content {
  gridelements_pi1 {
    20 {
      10 {
        setup {
          t3vBaseSection < lib.gridelements.defaultGridSetup
          t3vBaseSection {
            cObject = FLUIDTEMPLATE
            cObject {
              file = {$plugin.tx_t3vbase.settings.gridElements.sectionGridElement.cObject.file}
              layoutRootPath = {$plugin.tx_t3vbase.settings.gridElements.sectionGridElement.cObject.layoutRootPath}
              partialRootPath = {$plugin.tx_t3vbase.settings.gridElements.sectionGridElement.cObject.partialRootPath}

              dataProcessing {
                10 = T3v\T3vBase\DataProcessing\GridElement\StyleProcessor
                10 {
                  default {
                    as = styles
                  }
                }
              }
            }

            outerWrap = {$plugin.tx_t3vbase.settings.gridElements.sectionGridElement.outerWrap}
          }
        }
      }
    }
  }
}
